{
  "manifest_version": 2,

  "name": "Time Wasted",
  "description": "This extension will let you know how much time you spend on individual websites.",
  "version": "1.0",

  "background": {
    "scripts": ["jquery-2.1.4.min.js","background.js"]
  },

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  
  "permissions": [
   "tabs", "http://*/*"
   ]

}
